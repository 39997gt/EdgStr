{
  "name": "edge-refactor",
  "version": "1.0.0",
  "description": "Edge-Refactor is a software refactoring infrastructure that automatically transforms a two-tier distributed application into its three-tier variant. Edge-Refactor employs powerful profiling as well as code transformation and generation techniques to preserve the original execution semantics, adaptively reduce response latency, and increase processing throughput. It also introduces replicated state, efficiently synchronized across the edge and the cloud layers by means of Conflict-Free Replicated Data Types (CRDT).",
  "scripts": {
    "installing-py": "pip2.7 install z3-solver colored slimit",
    "EdgeRefactoring": "node src/js/extractSubject.js $RECORD && node src/js/jalangi2/src/js/commands/jalangi.js --inlineIID --inlineSource --analysis src/js/jalangi2/src/js/sample_analyses/ChainedAnalyses.js --analysis src/js/jalangi2/src/js/runtime/SMemory.js --analysis src/js/jalangi2/src/js/runtime/analysisCallbackTemplate_debug.js $SERVER ./Subject.json"
  },
